[user]
	email = vaikunth@tamu.edu
	name = Vaikunth Thukral
[color]
        branch = auto
        diff = auto
        interactive = auto
        status = auto
[alias]
        st = status
        resetorigin = "!f() { git fetch origin && git reset --hard @{u} && git clean -f -d; }; f"
        logfull = log --graph --decorate
        rebasemaster = !git fetch origin master && git rebase -i origin/master
[push]
        default = simple
[core]
        editor = emacs 
	excludesfile = /home/qserv/.gitignore
